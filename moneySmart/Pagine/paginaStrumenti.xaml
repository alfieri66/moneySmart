             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             x:Class="moneySmart.Pagine.paginaStrumenti">
    <ContentPage.Content>
        <StackLayout Padding="10">

            <Button x:Name="btnCreaDatabase"  Clicked="btnCreaDatabase_Click" Text="Cancella l'archivio" CornerRadius="10" BackgroundColor="#e9b006" TextColor="#FFFFFF" />
